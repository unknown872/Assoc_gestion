@model appli_gest_assoc.Models.TypeDepense

@{
    ViewData["Title"] = "Delete";
}

<div class="row">
    <div class="col-sm-12">
        <div class="panel panel-default">
            <div class="panel-heading text-center"><h3 class="panel-title">Suppression</h3></div>
            <div class="panel-body">
                <dl class="row">
                    <dt class="col-sm-2">
                        Libellé :
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Libelle)
                    </dd>
                </dl>

                <form asp-action="Delete">
                    <input type="hidden" asp-for="IdTypeDepense" />
                    <input type="submit" value="Supprimer" class="btn btn-danger" />
                </form>
            </div>
        </div>
    </div>
</div>
